          "fullScreen": "zoom", 
          "cached_js": " // #####################################################\n // #  IamHere NSB Version 1.0 .. 4/6/2017\n // #  Jacky Lau ... jackylau@yahoo.com\n // #\n // #  ToDo !!!                   \n // #  Replace \"apiKey\" to GoogleMap.apiKey (ggMap)\n // #  Fill MySQL Database info to Config.php\n // #\n // #  \u628a GoogleMap \u7684 apiKey, \u66f4\u6539\u70ba\u81ea\u5df1\u7684 \"apiKey\" \n // #  \u65bc Config.php \u586b\u5165\u81ea\u5df1\u7684 MySQL Database \u8cc7\u8a0a\n // #\n // #####################################################\n\n var v_ChineseMsg; // \u4e2d\u6587\u986f\u793a\n var v_ChineseInp; // \u4e2d\u6587\u986f\u793a\n var v_ChineseErr; // \u4e2d\u6587\u986f\u793a  \n var v_Chinese; // \u4e2d\u6587\u986f\u793a\n var n_Timer; //\u8a08\u6642\u9593\u9694\n var v_Count; //\u8a08\u6642\u986f\u793a\u6b21\u6578\n var c_Name = createMultiDimArray('4'); //\u7528\u6236\u540d\u7a31\n var n_NameNum; //\u4e4b\u6309\u4e0b\u7684\u7528\u6236\u540d\u7a31\u6309\u9215\u865f\n var n_TouchTimer; //\u9023\u7e8c\u6309\u4e0b\u6309\u9215\u7684\u8a08\u6642\u66ab\u5b58\n var c_Msg; // AJAX \u7684\u8f38\u51fa\u5165\u6578\u64da\n var MarkerLocal; // \u672c\u5730\u4e4b\u5730\u5716\u91dd\n var MarkerRemote; // \u5c0d\u65b9\u4e4b\u5730\u5716\u91dd\n var PointLocal; // \u672c\u5730\u5c0d\u4e0a\u4e00\u6b21\u986f\u793a\u7684\u4f4d\u7f6e\n var PointRemote; // \u5c0d\u65b9\u5c0d\u4e0a\u4e00\u6b21\u986f\u793a\u7684\u4f4d\u7f6e\n\n function Main() {\n     // \u7a0b\u5f0f\u958b\u59cb\n     var MyName;\n     // \u82e5\u592a\u7d30 Display, \u4e0d\u53ef\u4ee5\u8f49\u70ba\u6a6b\n     if (screen.width < 560) {\n         screen.orientation = \"portrait\";\n         screen.orientation.lock(\"portrait\");\n     }\n\n     // \u8a2d\u5b9a\u4e2d\u6587\u986f\u793a\n     if (Left(LCase(navigator.language), 3) == \"zh-\") {\n         v_Chinese = true;\n         v_ChineseMsg = \"\u8acb\u5148\u8f38\u5165\u540d\u7a31\";\n         v_ChineseInp = \"\u8f38\u5165\u540d\u5b57 \";\n         v_ChineseErr = \"\u672a\u53d6\u5f97 GPS \u4f4d\u7f6e\u8cc7\u8a0a\" + '\\n' + \"\u8acb\u958b\u555f\u8a2d\u5099\u4e2d\u7684 GPS\";\n         lblMinute.Caption = \"\u5b9a\u6642\u66f4\u65b0\u9593\u683c  (1-600 \u5206\u937e)\";\n         txtMinute.placeholder = \"\u5167\u5b9a (30) \u5206\u937e\";\n         lblAuto.Caption = \"\u81ea\u52d5\u66f4\u65b0\";\n         //rbAuto.items = \"\u505c\u6b62,\u958b\u59cb\"\n         lblNameSendH.Caption = \"\u767c\u51fa\u4e4b\u7a31\u865f (ID)\";\n         lblNameSendM.Caption = \"\u81ea\u52d5\u586b\u5beb\u7a31\u865f (\u5c0d\u65b9)\";\n         lblNameSendF.Caption = \"\u5148\u6309\u4e09\u4e0b\u5f8c\u518d\u9577\u6309 - \u8a2d\u5b9a\u81ea\u52d5\u7a31\u865f\";\n         txtNameSend.placeholder = \"\u8f38\u5165\u767c\u51fa\u7a31\u865f\u540d\u5b57\";\n         lblRaw.Caption = \"\u539f\u59cb\u8cc7\u6599\u8a0a\u606f\";\n         btHide.value = \"\u505c\u6b62\u8f38\u51fa - \u522a\u9664\u4f4d\u7f6e\u8a0a\u606f\";\n         btSend.value = \"\u8f38\u51fa\u73fe\u5728\u4f4d\u7f6e \u4e00\u6b21\";\n         lblNameGet.Caption = \"\u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e \u4e4b \u7a31\u865f\";\n         txtShowGet.placeholder = \"\u8f38\u5165\u5c0d\u65b9\u7a31\u865f\u540d\u5b57\";\n         btShow.value = \"\u986f\u793a\u4f4d\u7f6e - \u5c0d\u65b9\";\n     } else {\n         v_Chinese = false;\n         v_ChineseMsg = \"Input Name first\";\n         v_ChineseInp = \"Input Name \";\n         v_ChineseErr = \"Can't get GPS info.\" + '\\n' + \"Open GPS First !\";\n     }\n\n     // \u53d6\u56de\u4e4b\u524d\u5b58\u4e0b\u7684\u540d\u5b57\n     txtPassword.hidden = true;\n     txtPassword.Visible = false;\n     txtPassword.value = localStorage.getItem(\"PassWord\");\n     txtNameSend.value = localStorage.getItem(\"NameSend\");\n     txtShowGet.value = localStorage.getItem(\"ShowGet\");\n\n     // \u53d6\u56de\u4e4b\u524d\u5b58\u4e0b\u7684\u540d\u5b57\u8a18\u61b6\n     MyName = \"\";\n     for (i = 1; i <= 4; i++) {\n         c_Name[i] = localStorage.getItem(\"Name\" + CStr(i));\n         if ((c_Name[i] == \"undefined\") || IsNull(c_Name[i])) {\n             c_Name[i] = \"\";\n         }\n         MyName = MyName + c_Name[i];\n     }\n     // \u82e5\u672a\u6709\u8f38\u5165, \u53d6\u5f97\u5167\u5b9a\u4e4b\u540d\u7a31  \n     if (MyName == \"\") {\n         vReq = ReadFile(\"MyName.php\");\n         if (vReq.status == 200) {\n             vName = Split(vReq.responseText, \",\");\n             for (i = 1; i <= 4; i++) {\n                 c_Name[i] = vName[i];\n                 localStorage.setItem(\"Name\" + CStr(i), c_Name[i]);\n             }\n         }\n     }\n\n     n_NameNum = 0;\n     v_Count = 0;\n\n     lblShow.hidden = true;\n\n     //ggMap.apiKey = \"\"\n\n }\n\n fmMain.onshow = function() {\n     //\u8868\u55ae\u8f09\u5165\n };\n\n //\u81ea\u52d5\u5b9a\u671f\u66f4\u65b0\u6309\u9215\n rbAuto.onchange = function() {\n     if (rbAuto.getValue(1)) {\n         //   \u505c\u6b62\u81ea\u52d5\u5b9a\u6642\u66f4\u65b0  \n         clearInterval(n_Timer);\n         txtNameSend.readOnly = false;\n         v_Count = 0;\n     } else {\n         if (txtNameSend.value == \"\") {\n             p_DisableAuto(true);\n         } else {\n             rbAuto.disabled = true;\n             //     \u66f4\u65b0\u9593\u683c\u6642\u9593      \n             if ((_jsCint(txtMinute.value) == 0) || (_jsCint(txtMinute.value) > 600)) {\n                 txtMinute.value = 30;\n             }\n             if (v_Count == 0) {\n                 v_Count = 1;\n             }\n             //     \u958b\u59cb\u81ea\u52d5\u5b9a\u6642\u66f4\u65b0\n             n_Timer = setInterval(p_GetLocation, (_jsCint(txtMinute.value) * 60000));\n             txtNameSend.readOnly = true;\n             btSend.onclick(); // \u6a21\u64ec\u5df2\u6309\u4e0b\u4e00\u6b21 \"Send One Time\" \u6309\u9215\n         }\n     }\n };\n\n // \u505c\u6b62\u8f38\u51fa\u4f4d\u7f6e\n btHide.onclick = function() {\n     txtPassword.hidden = true;\n     txtPassword.Visible = false;\n     txtNameSend.value = Trim(txtNameSend.value);\n     if (txtNameSend.value != \"\") {\n         p_DisableAuto(false);\n         btHide.disabled = true;\n         rbAuto.value = 1;\n         v_Count = 0;\n         txtRaw.value = \"\";\n\n         txtPassword.value = Trim(txtPassword.value);\n         MyJSON = {\n             username: txtNameSend.value,\n             password: txtPassword.value\n         };\n         //   \u66f4\u65b0 MySQL \u8cc7\u6599\u5eab    \n         c_Msg = Ajax(\"ReadMySQL.php?mode=hidden\", \"POST\", (JSON.stringify(MyJSON)));\n     }\n\n     if ((lblShow.hidden) && (IsNull(MarkerLocal) == false)) {\n         MarkerLocal.setMap(null);\n         MarkerLocal = null;\n         PointLocal = null;\n     }\n };\n\n // \u8f38\u51fa\u4e00\u6b21\u4f4d\u7f6e\u8cc7\u8a0a (Send One Time)\n btSend.onclick = function() {\n     txtPassword.hidden = true;\n     txtPassword.Visible = false;\n     if (IsNull(MarkerLocal) == false) {\n         MarkerLocal.setMap(null);\n         MarkerLocal = null;\n         PointLocal = null;\n     }\n     if (txtNameSend.value == \"\") {\n         p_DisableAuto(true);\n     } else {\n         btSend.disabled = true;\n         txtRaw.value = \"\";\n         btHide.disabled = false;\n         lblShow.hidden = true;\n         p_GetLocation();\n     }\n };\n\n // \u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e\u8cc7\u8a0a\n btShow.onclick = function() {\n     txtShowGet.value = Trim(txtShowGet.value);\n     if (IsNull(MarkerRemote) == false) {\n         MarkerRemote.setMap(null);\n         MarkerRemote = null;\n         PointRemote = null;\n     }\n\n     if (txtShowGet.value == \"\") {\n         p_DisableAuto(true);\n     } else {\n         btShow.disabled = true;\n         if (lblShow.hidden == false) {\n             //     \u82e5\u6b63\u5728\u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e, \u5148\u628a\u6240\u6709\u986f\u793a\u91cd\u7f6e    \n             lblShow.hidden = true;\n             txtRaw.value = \"\";\n         }\n         MyJSON = {\n             username: txtShowGet.value,\n             lastgetip: \"0.0.0.0\"\n         };\n         //   \u5230\u7db2\u7ad9\u5f9e MySQL \u8cc7\u6599\u5eab\u53d6\u5f97\u4f4d\u7f6e\u8cc7\u8a0a\n         c_Msg = Ajax(\"ReadMySQL.php?mode=get\", \"POST\", (JSON.stringify(MyJSON)), f_AjaxCompletePOST);\n     }\n };\n\n // \u5df2\u5f9e\u7db2\u9801\u53d6\u5f97\u5c0d\u65b9\u5730\u5716\u6578\u64da \n function f_AjaxCompletePOST() {\n     if ((c_Msg.status == 200) && (Len(c_Msg.responseText) > 30)) {\n         vResult = Split(c_Msg.responseText, \",\");\n         vResult[0] = FormatDateTime(vResult[0], 10) + \" \" + FormatDateTime(vResult[0], 4);\n         //vResult(1) = Left(Trim(vResult(1)), (Len(vResult(1))-5))\n         //   \u986f\u793a\u8cc7\u8a0a\u6587\u5b57\n         txtRaw.value = vResult[0] + '\\n' + vResult[1] + \"  Log\" + '\\n' + vResult[2] + \"  Lat\" + '\\n' + Trim(vResult[3]) + \"  Accuracy\" + '\\n' + txtShowGet.value;\n         p_ShowPoint(vResult[1], vResult[2], true);\n         lblShow.hidden = false;\n     }\n     setTimeout(p_ResetBT, 5000);\n }\n\n\n //\u6c92\u6709\u8f38\u5165\u7528\u6236\u540d\u7a31\n function p_DisableAuto(vNoName) {\n     clearInterval(n_Timer);\n     txtNameSend.readOnly = false;\n     rbAuto.setValue(1, true);\n     rbAuto.setValue(2, false);\n     if (vNoName) {\n         NSB.MsgBox(v_ChineseMsg, 0, \"I am Here\");\n     }\n }\n\n //\u5f9e GPS \u53d6\u81ea\u8eab\u4f4d\u7f6e\u8cc7\u8a0a\n function p_GetLocation() {\n     navigator.geolocation.getCurrentPosition(p_ShowGeolocation, p_ErrorCallBack);\n }\n\n //\u4e0d\u80fd\u53d6\u5f97 GPS \u4f4d\u7f6e\u8cc7\u8a0a\n function p_ErrorCallBack(vError) {\n     txtRaw.value = v_ChineseErr;\n     setTimeout(p_ResetBT, 5000);\n }\n\n\n //\u5df2\u53d6\u5f97 GPS \u81ea\u8eab\u4f4d\u7f6e\u8cc7\u8a0a, \u986f\u793a\n function p_ShowGeolocation(vLocation) {\n     var MyJSON; // \u532f\u51fa\u7684\u8cc7\u6599 JSON\n\n     v_Date = FormatDateTime(DateAdd(\"s\", 0, new Date()), 10) + \" \" + FormatDateTime(DateAdd(\"s\", 0, new Date()), 4); // \u65e5\u671f\n\n     v_Long = Round(vLocation.coords.longitude, 9); // \u4f4d\u7f6e \u5317\u7def\n     v_Lat = Round(vLocation.coords.latitude, 9); // \u4f4d\u7f6e \u6771\u7d93\n     v_Acc = Round(vLocation.coords.accuracy, 0); // \u4f4d\u7f6e \u6e96\u78ba\u5ea6\n\n     txtRaw.value = v_Date + '\\n' + v_Long + \"  Log\" + '\\n' + v_Lat + \"  Lat\" + '\\n' + v_Acc + \"  Accuracy\";\n     if (v_Count > 0) {\n         //   \u81ea\u52d5\u7684\u6b21\u6578    \n         txtRaw.value = txtRaw.value + '\\n' + v_Count + \"  Count\";\n         v_Count = v_Count + 1;\n     }\n\n     lblShow.hidden = true;\n     p_ShowPoint(vLocation.coords.longitude, vLocation.coords.latitude, false);\n\n     txtNameSend.value = Trim(txtNameSend.value);\n     if (txtNameSend.value != \"\") {\n         txtPassword.value = Trim(txtPassword.value);\n         MyJSON = {\n             username: txtNameSend.value,\n             logdatetime: v_Date,\n             longitude: v_Long,\n             latitude: v_Lat,\n             accuracy: v_Acc,\n             sendip: \"0.0.0.0\",\n             password: txtPassword.value\n         };\n         //   \u66f4\u65b0 MySQL \u8cc7\u6599\u5eab, \u5132\u5b58\u73fe\u6709\u540d\u7a31\u7684\u4f4d\u7f6e\n         c_Msg = Ajax(\"ReadMySQL.php?mode=send\", \"POST\", (JSON.stringify(MyJSON)), f_AjaxCheckDoubleName);\n     }\n     setTimeout(p_ResetBT, 5000);\n }\n\n // \u5f9e\u7db2\u9801\u5b58\u56de\u7684\u662f\u5426\u5df2\u6709\u76f8\u540c\u540d\u7a31\u7684\u8cc7\u8a0a \n function f_AjaxCheckDoubleName() {\n     if ((c_Msg.status == 200)) {\n         vResult = Split(c_Msg.responseText, \",\");\n         if (Trim(vResult[0]) == \"NewName\") {\n             //     \u65b0\u540d\u5b57, \u628a Password (ID) \u5b58\u8d77      \n             txtPassword.value = Trim(vResult[1]);\n             localStorage.setItem(\"PassWord\", txtPassword.value);\n         } else if (Trim(vResult[0]) == \"NotSame\") {\n             //     \u76f8\u540c\u540d\u5b57, \u4f46 Password (ID) \u4e0d\u4e00\u6a23, \u5217\u51fa Password (ID), \u53ca\u522a\u9664\u525b\u986f\u793a\u7684\u4f4d\u7f6e      \n             txtRaw.value = \"\";\n             if (IsNull(MarkerLocal) == false) {\n                 MarkerLocal.setMap(null);\n                 MarkerLocal = null;\n                 PointLocal = null;\n             }\n             if (v_Chinese) {\n                 NSB.MsgBox(\"\u7a31\u865f \" + txtNameSend.value + \" \u5df2\u88ab\u4f7f\u7528\u4e2d\" + '\\n' + \"\u8acb\u8f49\u7528\u53e6\u4e00\u500b\u7a31\u865f\" + '\\n' + \"\u82e5\u78ba\u5b9a\u8981\u7528 \" + txtNameSend.value + \" \u4f5c\u7a31\u865f\" + '\\n' + \"\u53ef\u5728\u7a31\u865f\u65c1\u4e4b ID \u4f4d\u7f6e\" + '\\n' + \"\u8f38\u5165\u7de8\u78bc  \" + Trim(vResult[1]) + '\\n' + \"\u4ee5\u5f37\u5236\u4f7f\u7528\u6b64\u7a31\u865f\", 0, \"I am Here\");\n             } else {\n                 NSB.MsgBox(\"Name \" + txtNameSend.value + \" is already used\" + '\\n' + \"please input another Name\" + '\\n' + \"If you still wanna this name\" + '\\n' + \"do input the code  \" + Trim(vResult[1]) + '\\n' + \"into ID box beside the Name\" + '\\n' + \"for force use Name \" + txtNameSend.value, 0, \"I am Here\");\n             }\n             txtPassword.value = \"\";\n             txtPassword.hidden = false;\n             txtPassword.Visible = true;\n         }\n     }\n }\n\n\n //\u986f\u793a\u6a19\u8a18\u65bc\u5730\u5716\u4e0a\n function p_ShowPoint(vLong, vLat, vOtherSide) {\n     var vPoint;\n\n     ggMap.mapOptions.longitude = vLong;\n     ggMap.mapOptions.latitude = vLat;\n     ggMap.mapOptions.zoom = 16;\n     ggMap.refresh();\n\n     vPoint = new google.maps.LatLng(vLat, vLong);\n\n     if (vOtherSide) {\n         PointRemote = vPoint;\n         MarkerRemote = ggMap.setMarker({\n             position: vPoint,\n             animation: google.maps.Animation.DROP\n         });\n         if (IsNull(PointLocal) == false) {\n             MarkerLocal = ggMap.setMarker({\n                 position: PointLocal,\n                 icon: {\n                     url: \"schools.png\",\n                     scaledSize: new google.maps.Size(32, 32)\n                 }\n             });\n         }\n     } else {\n         PointLocal = vPoint;\n         if (IsNull(PointRemote) == false) {\n             MarkerRemote = ggMap.setMarker({\n                 position: PointRemote\n             });\n         }\n         //   \u5982\u679c\u662f Apple \u6a5f, \u4e0d\u53ef\u4f7f\u7528 Google \u7ad9\u5167\u7684 icon !!! (\u552f\u6709\u628a ICON \u8907\u88fd\u5728\u672c\u7ad9\u5167) ... BUG !    \n         //MarkerLocal = ggMap.setMarker({position: vPoint, icon: {url:\"http://maps.google.com/mapfiles/kml/paddle/schools.png\", scaledSize: new google.maps.Size(32, 32)}})\n         MarkerLocal = ggMap.setMarker({\n             position: vPoint,\n             icon: {\n                 url: \"schools.png\",\n                 scaledSize: new google.maps.Size(32, 32)\n             },\n             animation: google.maps.Animation.DROP\n         });\n     }\n\n     vPoint = null;\n }\n\n //\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u6309\u9215\n rbName.onchange = function() {\n     // \u8a2d\u5b9a\u70ba\u8f38\u5165\u540d\u7a31\n     txtShowGet.value = c_Name[rbName.value()];\n     localStorage.setItem(\"ShowGet\", txtShowGet.value);\n };\n\n //\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u8a08\u6642\u4ee5\u4f5c\u5132\u5b58\n rbName.ontouchstart = function() {\n     if (rbName.value() == n_NameNum) {\n         //   \u9023\u7e8c\u6309\u4e0b\u8a08\u6642 (\u5167\u5b9a\u70ba 1500, 1.5 \u79d2)    \n         n_TouchTimer = setTimeout(p_SetName, 1500);\n     }\n     n_NameNum = rbName.value();\n };\n\n //\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u8a08\u6642\u505c\u6b62\n rbName.ontouchend = function() {\n     clearTimeout(n_TouchTimer);\n };\n\n //\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u6309\u9215\u540c\u6642\u6309\u4e0b Ctrl \u9375 (PC \u7db2\u9801\u4e0a) (\u7b49\u540c\u65bc\u9577\u6309\u540d\u7a31\u8a18\u61b6\u6309\u9215)\n rbName.onclick = function(e) {\n     if (e.ctrlKey) {\n         n_NameNum = rbName.value();\n         p_SetName();\n     }\n };\n\n //\u8a2d\u5b9a\u540d\u7a31\u8a18\u61b6\u6309\u9215\u4e4b\u540d\u5b57\n function p_SetName() {\n     var returnValue = \"\";\n     if (c_Name[n_NameNum] == null) {\n         c_Name[n_NameNum] = \"\";\n     }\n     c_Name[n_NameNum] = prompt(v_ChineseInp + CStr(n_NameNum), c_Name[n_NameNum]);\n     if (c_Name[n_NameNum] == null) {\n         c_Name[n_NameNum] = \"\";\n     }\n     c_Name[n_NameNum] = Trim(c_Name[n_NameNum]);\n     if (Len(c_Name[n_NameNum]) > 20) {\n         c_Name[n_NameNum] = Left(c_Name[n_NameNum], 20);\n     }\n     localStorage.setItem(\"Name\" + n_NameNum, c_Name[n_NameNum]);\n }\n\n\n txtNameSend.onchange = function() {\n     localStorage.setItem(\"NameSend\", txtNameSend.value);\n };\n\n txtShowGet.onchange = function() {\n     localStorage.setItem(\"ShowGet\", txtShowGet.value);\n };\n\n txtPassword.onchange = function() {\n     localStorage.setItem(\"PassWord\", txtPassword.value);\n };\n\n function p_ResetBT() {\n     rbAuto.disabled = false;\n     btSend.disabled = false;\n     btShow.disabled = false;\n }", 
