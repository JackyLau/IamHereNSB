          "script": "<?\n// \u9664\u932f\u7528, \u4e0a\u7dda\u6642\u53ef Remark \u4ee5\u4e0b\u5169\u884c\n//error_reporting(E_ALL);\n//ini_set('display_errors', 1);\n\n$MyIP = $_SERVER['REMOTE_ADDR'];\n\r\n// \u8cc7\u6599\u5eab\u540d\u7a31\u53ca\u5bc6\u78bc\u8a2d\u5b9a\r\nif (file_exists('MyConfig.php')) {\r\n  include_once('MyConfig.php');\r\n} else {\r\n  include_once('Config.php');\r\n}\r\n\n$datatableLOC = 'iamhere_location';  // \u4e3b\u8981\u5730\u57df\u8cc7\u6599\u8868\n$datatableNAME = 'iamhere_username';  // \u7528\u6236\u59d3\u540d\u5217\u8868\n\n// \u6e2c\u8a66\u6709\u6c92\u6709\u9019\u500b Table, \u5982\u679c\u6c92\u6709, \u50b3\u56de True, \u5982\u679c\u6c92\u6709 False\nfunction f_ChkTable($table) {\n\tGLOBAL $link;\n\t$sql = \"SHOW TABLES LIKE '$table'\";\n\t$result = mysqli_query($link, $sql);\n\treturn $result->num_rows > 0;\n}\n\n\n// \u6253\u958b\u9023\u63a5 SQL \u8cc7\u6599\u5eab\u53ca\u6253\u958b\u4e00\u500b DataBase File\nGLOBAL $link;\n$link = @new mysqli(C_db_domain, C_db_user, C_db_pass, C_db_file);\n// \u627e\u4e0d\u5230\u8cc7\u6599\u5eab, \u96e2\u958b\nif ($link->connect_errno) { exit(); }\n// \u652f\u6301 UTF8 \u4e2d\u6587 ... \u9054\u81f3\u4e2d\u6587\u4e0d\u6703\u986f\u793a\u70ba '?????' \u5b57\u6a23 \nmysqli_set_charset($link, \"UTF8\");  \n\n\n// \u5982\u679c\u6c92\u6709\u9019\u500b DataBase Table \u5b58\u5728, \u81ea\u52d5\u958b\u4e00\u500b\nif (!f_ChkTable($datatableLOC)) {\n\t$sql = \"CREATE TABLE $datatableLOC (\n\tid int(8) UNSIGNED AUTO_INCREMENT PRIMARY KEY,\n\tusername char(20),\n\tlogdatetime datetime,\n\tlongitude decimal(13,9),\n\tlatitude decimal(13,9),\n\taccuracy int(11),\n\tshowout tinyint(1),\n\tsendip char(15),\n\tlastgetip char(15),\n\tgetcount int(11) \n\t) DEFAULT CHARSET=utf8\";\n\tmysqli_query($link, $sql);\n}\n\n\n// \u5982\u679c\u6c92\u6709\u9019\u500b DataBase Table \u5b58\u5728, \u81ea\u52d5\u958b\u4e00\u500b\nif (!f_ChkTable($datatableNAME)) {\n\t$sql = \"CREATE TABLE $datatableNAME (\n\tid int(8) UNSIGNED AUTO_INCREMENT PRIMARY KEY,\n\tusername char(20),\n\tsimplepassword char(10),\n\tcreatedatetime datetime,\n\tlastdatetime datetime,\n\tupdatecount int(11)\n\t) DEFAULT CHARSET=utf8\";\n\tmysqli_query($link, $sql);\n}\n\n\n$DataIn = json_decode(file_get_contents('php://input'), true);\n\nif (($_GET['mode'] == 'send') || ($_GET['mode'] == 'hidden')) {\n//\u5132\u5b58\u5730\u57df\u4f4d\u7f6e\u8cc7\u6599\n  $sql = '';\n  foreach($DataIn as $vKey => $vValue) {\n    if ($vKey == 'password') {\n      $vPassword = $vValue;\n    } else {\n      if ($vKey == 'username') {$vUsername = $vValue;}\n      if ($vKey == 'logdatetime') {$vLogDateTime = $vValue;}\n      if (($vKey == 'sendip') && ($vValue == '0.0.0.0')) { $vValue = $MyIP; }\n      $sql = $sql . \"$vKey = '$vValue', \";   \n    }\n  }\n\n//\u53d6\u5f97\u4e4b\u524d\u7684\u540d\u5b57(\u82e5\u6709), \u6838\u5c0d\u662f\u5426\u8207 Password (ID) \u76f8\u540c\n  $result = mysqli_query($link, \"SELECT simplepassword FROM $datatableNAME WHERE username = '$vUsername'\");\n\n  if (mysqli_num_rows($result) >= 1) {\n//\u627e\u5230\u540d\u5b57, \u6838\u5c0d\u662f\u5426\u8207 Password (ID) \u76f8\u540c\n    $RowOut = mysqli_fetch_assoc($result);\n    if ($RowOut['simplepassword'] == $vPassword) {\n//    \u76f8\u540c, \u628a\u4e4b\u524d\u8a72\u7528\u6236\u7684\u8cc7\u8a0a, \u5148\u6539\u70ba\u4e0d\u5217\u51fa\n      mysqli_query($link, \"UPDATE $datatableLOC SET showout = false WHERE username = '$vUsername'\");\n    } else {\n//    \u4e0d\u540c, \u96e2\u958b, \u53ca\u544a\u77e5 Password (ID)\n      echo \"NotSame,\" . $RowOut['simplepassword'];\n      mysqli_free_result($result);\n      mysqli_close($link);\n      exit();\n    }\n  } else {\n//  \u627e\u4e0d\u5230\u540d\u5b57, \u65b0\u589e\u4e00\u500b Password (ID), \u5132\u5b58\u540d\u5b57 \u53ca \u50b3\u56de Password (ID) \n    $vPassword = rand(1000,9999);\n    echo \"NewName,\" . $vPassword;\n    mysqli_query($link, \"INSERT INTO $datatableNAME SET username = '$vUsername', simplepassword = '$vPassword', createdatetime = '$vLogDateTime', updatecount = 0\");\n  }\n  \n  if ($_GET['mode'] == 'send') {\n//  \u628a\u8a72\u7528\u6236\u6700\u65b0\u7684\u8cc7\u8a0a, \u5132\u5b58\u53ca\u8a2d\u70ba\u5217\u51fa\n    mysqli_query($link, \"UPDATE $datatableNAME SET lastdatetime = '$vLogDateTime', updatecount = (updatecount + 1) WHERE username = '$vUsername'\");\n\n    $sql = \"INSERT INTO $datatableLOC SET \" . $sql . \"showout = true, getcount = 0\";\n    mysqli_query($link, $sql);\n  }\n  \n} elseif ($_GET['mode'] == 'get') {\n//\u53d6\u5f97\u5730\u57df\u4f4d\u7f6e\u8cc7\u6599  \n  foreach($DataIn as $vKey => $vValue) {\n    if ($vKey == 'username') {$vUsername = $vValue;}\n    if ($vKey == 'lastgetip') {\n      $vLastGetIP = $vValue;\n      if ($vLastGetIP == '0.0.0.0') { $vLastGetIP = $MyIP; }\n    }\n  }\n\n  $sql = \"SELECT * FROM $datatableLOC WHERE ((username = '$vUsername') AND (showout))\";\n  $result = mysqli_query($link, $sql);\n\n//\u6709\u5730\u57df\u4f4d\u7f6e\u8cc7\u6599, \u767c\u56de\u8cc7\u8a0a\u81f3\u539f\u672c\u7a0b\u5f0f\n  if (mysqli_num_rows($result) >= 1) {\n    $RowOut = mysqli_fetch_assoc($result);\n    echo $RowOut['logdatetime'] . ',' . $RowOut['longitude'] . ',' . $RowOut['latitude'] . ',' . $RowOut['accuracy'];\n    \n//  \u8a18\u4e0b IP, \u53ca\u8a08\u6578\u52a0\u4e00\n    mysqli_query($link, \"UPDATE $datatableLOC SET lastgetip = '$vLastGetIP', getcount = (getcount + 1) WHERE ((username = '$vUsername') AND (showout))\");\n  }\n\n  mysqli_free_result($result);\n}\n\nmysqli_close($link);\n?>\n", 
