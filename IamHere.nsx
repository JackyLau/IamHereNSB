          "script": "' #####################################################\r\n' #  IamHere NSB Version 1.0 .. 4/6/2017\r\n' #  Jacky Lau ... jackylau@yahoo.com\r\n' #\r\n' #  ToDo !!!                   \r\n' #  Replace \"apiKey\" to GoogleMap.apiKey (ggMap)\r\n' #  Fill MySQL Database info to Config.php\r\n' #\r\n' #  \u628a GoogleMap \u7684 apiKey, \u66f4\u6539\u70ba\u81ea\u5df1\u7684 \"apiKey\" \r\n' #  \u65bc Config.php \u586b\u5165\u81ea\u5df1\u7684 MySQL Database \u8cc7\u8a0a\r\n' #\r\n' #####################################################\r\n\r\nDim v_ChineseMsg ' \u4e2d\u6587\u986f\u793a\r\nDim v_ChineseInp ' \u4e2d\u6587\u986f\u793a\r\nDim v_ChineseErr ' \u4e2d\u6587\u986f\u793a  \r\nDim v_Chinese ' \u4e2d\u6587\u986f\u793a\r\nDim n_Timer '\u8a08\u6642\u9593\u9694\r\nDim v_Count '\u8a08\u6642\u986f\u793a\u6b21\u6578\r\nDim c_Name(4) '\u7528\u6236\u540d\u7a31\r\nDim n_NameNum  '\u4e4b\u6309\u4e0b\u7684\u7528\u6236\u540d\u7a31\u6309\u9215\u865f\r\nDim n_TouchTimer  '\u9023\u7e8c\u6309\u4e0b\u6309\u9215\u7684\u8a08\u6642\u66ab\u5b58\r\nDim c_Msg  ' AJAX \u7684\u8f38\u51fa\u5165\u6578\u64da\r\nDim MarkerLocal  ' \u672c\u5730\u4e4b\u5730\u5716\u91dd\r\nDim MarkerRemote  ' \u5c0d\u65b9\u4e4b\u5730\u5716\u91dd\r\nDim PointLocal  ' \u672c\u5730\u5c0d\u4e0a\u4e00\u6b21\u986f\u793a\u7684\u4f4d\u7f6e\r\nDim PointRemote  ' \u5c0d\u65b9\u5c0d\u4e0a\u4e00\u6b21\u986f\u793a\u7684\u4f4d\u7f6e\r\n\r\nSub Main()\r\n' \u7a0b\u5f0f\u958b\u59cb\r\n  Dim MyName\r\n' \u82e5\u592a\u7d30 Display, \u4e0d\u53ef\u4ee5\u8f49\u70ba\u6a6b\r\n  if screen.width < 560 Then\r\n    Set screen.orientation = \"portrait\"\r\n    screen.orientation.lock(\"portrait\")\r\n  End if\r\n\r\n' \u8a2d\u5b9a\u4e2d\u6587\u986f\u793a\r\n  if Left(LCase(navigator.language),3) = \"zh-\" Then\r\n    v_Chinese = True\r\n    v_ChineseMsg = \"\u8acb\u5148\u8f38\u5165\u540d\u7a31\"\r\n    v_ChineseInp = \"\u8f38\u5165\u540d\u5b57 \"\r\n    v_ChineseErr = \"\u672a\u53d6\u5f97 GPS \u4f4d\u7f6e\u8cc7\u8a0a\" & vbCRLF & \"\u8acb\u958b\u555f\u8a2d\u5099\u4e2d\u7684 GPS\"\r\n    lblMinute.Caption = \"\u5b9a\u6642\u66f4\u65b0\u9593\u683c  (1-600 \u5206\u937e)\"\r\n    txtMinute.placeholder = \"\u5167\u5b9a (30) \u5206\u937e\"\r\n    lblAuto.Caption = \"\u81ea\u52d5\u66f4\u65b0\"\r\n    'rbAuto.items = \"\u505c\u6b62,\u958b\u59cb\"\r\n    lblNameSendH.Caption = \"\u767c\u51fa\u4e4b\u7a31\u865f (ID)\"\r\n    lblNameSendM.Caption = \"\u81ea\u52d5\u586b\u5beb\u7a31\u865f (\u5c0d\u65b9)\"\r\n    lblNameSendF.Caption = \"\u5148\u6309\u4e09\u4e0b\u5f8c\u518d\u9577\u6309 - \u8a2d\u5b9a\u81ea\u52d5\u7a31\u865f\"\r\n    txtNameSend.placeholder = \"\u8f38\u5165\u767c\u51fa\u7a31\u865f\u540d\u5b57\"\r\n    lblRaw.Caption = \"\u539f\u59cb\u8cc7\u6599\u8a0a\u606f\"\r\n    btHide.value = \"\u505c\u6b62\u8f38\u51fa - \u522a\u9664\u4f4d\u7f6e\u8a0a\u606f\"\r\n    btSend.value = \"\u8f38\u51fa\u73fe\u5728\u4f4d\u7f6e \u4e00\u6b21\"\r\n    lblNameGet.Caption = \"\u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e \u4e4b \u7a31\u865f\"\r\n    txtShowGet.placeholder = \"\u8f38\u5165\u5c0d\u65b9\u7a31\u865f\u540d\u5b57\"\r\n    btShow.value = \"\u986f\u793a\u4f4d\u7f6e - \u5c0d\u65b9\"\r\n  else\r\n    v_Chinese = False\r\n    v_ChineseMsg = \"Input Name first\"\r\n    v_ChineseInp = \"Input Name \"\r\n    v_ChineseErr = \"Can't get GPS info.\" & vbCRLF & \"Open GPS First !\"\r\n  End if\r\n\r\n' \u53d6\u56de\u4e4b\u524d\u5b58\u4e0b\u7684\u540d\u5b57\r\n  txtPassword.hidden = True\r\n  txtPassword.Visible = False\r\n  txtPassword.value = localStorage.getItem(\"PassWord\")\r\n  txtNameSend.value = localStorage.getItem(\"NameSend\")\r\n  txtShowGet.value = localStorage.getItem(\"ShowGet\")\r\n\r\n' \u53d6\u56de\u4e4b\u524d\u5b58\u4e0b\u7684\u540d\u5b57\u8a18\u61b6\r\n  MyName = \"\"\r\n  for i = 1 To 4\r\n    c_Name(i) = localStorage.getItem(\"Name\" & CStr(i))\r\n    if (c_Name(i) = \"undefined\") Or IsNull(c_Name(i)) Then c_Name(i) = \"\"\r\n    MyName = MyName & c_Name(i)\r\n  Next\r\n' \u82e5\u672a\u6709\u8f38\u5165, \u53d6\u5f97\u5167\u5b9a\u4e4b\u540d\u7a31  \r\n  if MyName = \"\" Then\r\n    vReq = ReadFile(\"MyName.php\")\r\n    if vReq.status = 200 Then\r\n      vName = Split(vReq.responseText, \",\")\r\n      for i = 1 To 4\r\n        c_Name(i) = vName(i)\r\n        localStorage.setItem(\"Name\" & CStr(i), c_Name(i))\r\n      Next\r\n    End if\r\n  End if\r\n    \r\n  n_NameNum = 0\r\n  v_Count = 0\r\n  \r\n  lblShow.hidden=True\r\n\r\n  'ggMap.apiKey = \"\"\r\n\r\nEnd Sub\r\n\r\nfunction fmMain_onshow()\r\n'\u8868\u55ae\u8f09\u5165\r\nEnd function\r\n\r\n'\u81ea\u52d5\u5b9a\u671f\u66f4\u65b0\u6309\u9215\r\nfunction rbAuto_onchange()\r\n  if rbAuto.getValue(1) Then\r\n'   \u505c\u6b62\u81ea\u52d5\u5b9a\u6642\u66f4\u65b0  \r\n    ClearInterval (n_Timer)\r\n    txtNameSend.readOnly = False\r\n    v_Count = 0    \r\n  else\r\n    if txtNameSend.value = \"\" Then\r\n      Call p_DisableAuto(True)\r\n    else\r\n      rbAuto.disabled = True\r\n'     \u66f4\u65b0\u9593\u683c\u6642\u9593      \r\n      if (CInt(txtMinute.value) = 0) Or (CInt(txtMinute.value) > 600) Then\r\n        txtMinute.value = 30\r\n      End if\r\n      if v_Count = 0 Then v_Count = 1\r\n'     \u958b\u59cb\u81ea\u52d5\u5b9a\u6642\u66f4\u65b0\r\n      n_Timer = SetInterval(p_GetLocation, (CInt(txtMinute.value) * 60000))\r\n      txtNameSend.readOnly = True\r\n      Call btSend_onclick  ' \u6a21\u64ec\u5df2\u6309\u4e0b\u4e00\u6b21 \"Send One Time\" \u6309\u9215\r\n    End if\r\n  End if\r\nEnd function\r\n\r\n' \u505c\u6b62\u8f38\u51fa\u4f4d\u7f6e\nfunction btHide_onclick()\r\n  txtPassword.hidden = True\r\n  txtPassword.Visible = False  \r\n  txtNameSend.value = Trim(txtNameSend.value)\r\n  if txtNameSend.value <> \"\" Then\r\n    Call p_DisableAuto(False)\r\n    btHide.disabled=True\r\n    rbAuto.value=1\r\n    v_Count = 0\r\n    txtRaw.value = \"\"\r\n    \r\n    txtPassword.value = Trim(txtPassword.value)\r\n    MyJSON = {username:txtNameSend.value, password:txtPassword.value}\r\n'   \u66f4\u65b0 MySQL \u8cc7\u6599\u5eab    \r\n    c_Msg = Ajax(\"ReadMySQL.php?mode=hidden\", \"POST\", (JSON.stringify(MyJSON)))\r\n  End if\r\n\r\n  if (lblShow.hidden) And (IsNull(MarkerLocal) = False) Then\r\n    MarkerLocal.setMap(null)\r\n    MarkerLocal = null\r\n    PointLocal = null\r\n  End if\r\nEnd function\n\r\n' \u8f38\u51fa\u4e00\u6b21\u4f4d\u7f6e\u8cc7\u8a0a (Send One Time)\nfunction btSend_onclick()\r\n  txtPassword.hidden = True\r\n  txtPassword.Visible = False\r\n  if IsNull(MarkerLocal) = False Then\r\n    MarkerLocal.setMap(null)\r\n    MarkerLocal = null\r\n    PointLocal = null\r\n  End if\r\n  if txtNameSend.value = \"\" Then\r\n    Call p_DisableAuto(True)\r\n  else\r\n    btSend.disabled = True\r\n    txtRaw.value = \"\"\r\n    btHide.disabled = False\r\n    lblShow.hidden = True\r\n    Call p_GetLocation\r\n  End if\r\nEnd function\n\r\n' \u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e\u8cc7\u8a0a\r\nfunction btShow_onclick()\r\n  txtShowGet.value = Trim(txtShowGet.value)\r\n  if IsNull(MarkerRemote) = False Then\r\n    MarkerRemote.setMap(null)\r\n    MarkerRemote = null\r\n    PointRemote = null\r\n  End if\r\n\r\n  if txtShowGet.value = \"\" Then\r\n    Call p_DisableAuto(True)\r\n  else\r\n    btShow.disabled = True\r\n    if lblShow.hidden = False Then\r\n'     \u82e5\u6b63\u5728\u986f\u793a\u5c0d\u65b9\u4f4d\u7f6e, \u5148\u628a\u6240\u6709\u986f\u793a\u91cd\u7f6e    \r\n      lblShow.hidden=True\r\n      txtRaw.value = \"\"\r\n    End if\r\n    MyJSON = {username:txtShowGet.value, lastgetip:\"0.0.0.0\"}\r\n'   \u5230\u7db2\u7ad9\u5f9e MySQL \u8cc7\u6599\u5eab\u53d6\u5f97\u4f4d\u7f6e\u8cc7\u8a0a\r\n    c_Msg = Ajax(\"ReadMySQL.php?mode=get\", \"POST\", (JSON.stringify(MyJSON)), f_AjaxCompletePOST)\r\n  End if\r\nEnd function\n\r\n' \u5df2\u5f9e\u7db2\u9801\u53d6\u5f97\u5c0d\u65b9\u5730\u5716\u6578\u64da \r\nfunction f_AjaxCompletePOST()\r\n  if (c_Msg.status = 200) And (Len(c_Msg.responseText) > 30) Then\r\n    vResult = Split(c_Msg.responseText, \",\")\r\n    vResult(0) = FormatDateTime(vResult(0), vbYYYYhyphenMMhyphenDD) & \" \" & FormatDateTime(vResult(0), vbShortTime)\r\n    'vResult(1) = Left(Trim(vResult(1)), (Len(vResult(1))-5))\r\n'   \u986f\u793a\u8cc7\u8a0a\u6587\u5b57\r\n    txtRaw.value = vResult(0) & vbCRLF & vResult(1) & \"  Log\" & vbCRLF & vResult(2) &  \"  Lat\" & vbCRLF & Trim(vResult(3)) &  \"  Accuracy\" & vbCRLF & txtShowGet.value\r\n    Call p_ShowPoint(vResult(1), vResult(2), True)\r\n    lblShow.hidden=False\r\n  End if\r\n  SetTimeout(p_ResetBT, 5000)\r\nEnd function  \r\n\r\n'\u6c92\u6709\u8f38\u5165\u7528\u6236\u540d\u7a31\r\nSub p_DisableAuto(vNoName)\r\n  ClearInterval(n_Timer)\r\n  txtNameSend.readOnly = False\r\n  rbAuto.setValue(1, True)\r\n  rbAuto.setValue(2, False)\r\n  if vNoName Then\r\n    MsgBox(v_ChineseMsg, 0, \"I am Here\")\r\n  End if\r\nEnd Sub\r\n\r\n'\u5f9e GPS \u53d6\u81ea\u8eab\u4f4d\u7f6e\u8cc7\u8a0a\r\nfunction p_GetLocation()\r\n  navigator.geolocation.getCurrentPosition(p_ShowGeolocation, p_ErrorCallBack)  \r\nEnd function\r\n\r\n'\u4e0d\u80fd\u53d6\u5f97 GPS \u4f4d\u7f6e\u8cc7\u8a0a\r\nfunction p_ErrorCallBack(vError)\r\n  txtRaw.value = v_ChineseErr\r\n  SetTimeout(p_ResetBT, 5000)\r\nEnd function  \r\n\r\n'\u5df2\u53d6\u5f97 GPS \u81ea\u8eab\u4f4d\u7f6e\u8cc7\u8a0a, \u986f\u793a\r\nfunction p_ShowGeolocation(vLocation)\r\n  Dim MyJSON  ' \u532f\u51fa\u7684\u8cc7\u6599 JSON\r\n  \r\n  v_Date = FormatDateTime(Now, vbYYYYhyphenMMhyphenDD) & \" \" & FormatDateTime(Now, vbShortTime)  ' \u65e5\u671f\r\n\r\n  v_Long = Round(vLocation.coords.longitude, 9)  ' \u4f4d\u7f6e \u5317\u7def\r\n  v_Lat = Round(vLocation.coords.latitude, 9)  ' \u4f4d\u7f6e \u6771\u7d93\r\n  v_Acc = Round(vLocation.coords.accuracy, 0)  ' \u4f4d\u7f6e \u6e96\u78ba\u5ea6\r\n\r\n  txtRaw.value = v_Date & vbCRLF & v_Long & \"  Log\" & vbCRLF & v_Lat &  \"  Lat\" & vbCRLF & v_Acc &  \"  Accuracy\"\r\n  if v_Count > 0 Then\r\n'   \u81ea\u52d5\u7684\u6b21\u6578    \r\n    txtRaw.value = txtRaw.value & vbCRLF & v_Count & \"  Count\"\r\n    v_Count = v_Count + 1  \r\n  End if\r\n\r\n  lblShow.hidden=True\r\n  Call p_ShowPoint(vLocation.coords.longitude, vLocation.coords.latitude, False)\r\n\r\n  txtNameSend.value = Trim(txtNameSend.value)\r\n  if txtNameSend.value <> \"\" Then\r\n    txtPassword.value = Trim(txtPassword.value)\r\n    MyJSON = {username:txtNameSend.value, logdatetime:v_Date,  longitude:v_Long, latitude:v_Lat, accuracy:v_Acc, sendip:\"0.0.0.0\", password:txtPassword.value}\r\n'   \u66f4\u65b0 MySQL \u8cc7\u6599\u5eab, \u5132\u5b58\u73fe\u6709\u540d\u7a31\u7684\u4f4d\u7f6e\r\n    c_Msg = Ajax(\"ReadMySQL.php?mode=send\", \"POST\", (JSON.stringify(MyJSON)), f_AjaxCheckDoubleName)\r\n  End if\r\n  SetTimeout(p_ResetBT, 5000)\r\nEnd function\r\n\r\n' \u5f9e\u7db2\u9801\u5b58\u56de\u7684\u662f\u5426\u5df2\u6709\u76f8\u540c\u540d\u7a31\u7684\u8cc7\u8a0a \r\nfunction f_AjaxCheckDoubleName()\r\n  if (c_Msg.status = 200) Then\r\n    vResult = Split(c_Msg.responseText, \",\")\r\n    if Trim(vResult(0)) = \"NewName\" Then\r\n'     \u65b0\u540d\u5b57, \u628a Password (ID) \u5b58\u8d77      \r\n      txtPassword.value = Trim(vResult(1))\r\n      localStorage.setItem(\"PassWord\", txtPassword.value)\r\n    else if Trim(vResult(0)) = \"NotSame\" Then\r\n'     \u76f8\u540c\u540d\u5b57, \u4f46 Password (ID) \u4e0d\u4e00\u6a23, \u5217\u51fa Password (ID), \u53ca\u522a\u9664\u525b\u986f\u793a\u7684\u4f4d\u7f6e      \r\n      txtRaw.value = \"\"\r\n      if IsNull(MarkerLocal) = False Then\r\n        MarkerLocal.setMap(null)\r\n        MarkerLocal = null\r\n        PointLocal = null\r\n      End if \r\n      if v_Chinese Then\r\n        MsgBox(\"\u7a31\u865f \" & txtNameSend.value & \" \u5df2\u88ab\u4f7f\u7528\u4e2d\" & vbCRLF & \"\u8acb\u8f49\u7528\u53e6\u4e00\u500b\u7a31\u865f\" & vbCRLF & \"\u82e5\u78ba\u5b9a\u8981\u7528 \"  & txtNameSend.value & \" \u4f5c\u7a31\u865f\" & vbCRLF & \"\u53ef\u5728\u7a31\u865f\u65c1\u4e4b ID \u4f4d\u7f6e\"  & vbCRLF & \"\u8f38\u5165\u7de8\u78bc  \" & Trim(vResult(1)) & vbCRLF & \"\u4ee5\u5f37\u5236\u4f7f\u7528\u6b64\u7a31\u865f\", 0, \"I am Here\")\r\n      else\r\n        MsgBox(\"Name \" & txtNameSend.value & \" is already used\" & vbCRLF & \"please input another Name\" & vbCRLF & \"If you still wanna this name\" & vbCRLF & \"do input the code  \"  & Trim(vResult(1)) & vbCRLF & \"into ID box beside the Name\" & vbCRLF & \"for force use Name \" & txtNameSend.value, 0, \"I am Here\")\r\n      End if  \r\n      txtPassword.value = \"\"\r\n      txtPassword.hidden = False\r\n      txtPassword.Visible = True\r\n    End if\r\n  End if\r\nEnd function  \r\n\r\n'\u986f\u793a\u6a19\u8a18\u65bc\u5730\u5716\u4e0a\r\nfunction p_ShowPoint(vLong, vLat, vOtherSide)\r\n  Dim vPoint\r\n  \r\n  ggMap.mapOptions.longitude = vLong\r\n  ggMap.mapOptions.latitude = vLat\r\n  ggMap.mapOptions.zoom = 16\r\n  ggMap.refresh()\r\n\r\n  vPoint = new google.maps.LatLng(vLat, vLong)\r\n\r\n  if vOtherSide Then\r\n    PointRemote = vPoint\r\n    if IsNull(PointLocal) = False Then MarkerLocal = ggMap.setMarker({position: PointLocal, icon: {url:\"schools.png\", scaledSize: new google.maps.Size(32, 32)}})\r\n    MarkerRemote = ggMap.setMarker({position: vPoint, animation: google.maps.Animation.DROP})\r\n  else\r\n    PointLocal = vPoint\r\n    if IsNull(PointRemote) = False Then MarkerRemote = ggMap.setMarker({position: PointRemote})\r\n'   \u5982\u679c\u662f Apple \u6a5f, \u4e0d\u53ef\u4f7f\u7528 Google \u7ad9\u5167\u7684 icon !!! (\u552f\u6709\u628a ICON \u8907\u88fd\u5728\u672c\u7ad9\u5167) ... BUG !    \r\n    'MarkerLocal = ggMap.setMarker({position: vPoint, icon: {url:\"http://maps.google.com/mapfiles/kml/paddle/schools.png\", scaledSize: new google.maps.Size(32, 32)}})\r\n    MarkerLocal = ggMap.setMarker({position: vPoint, icon: {url:\"schools.png\", scaledSize: new google.maps.Size(32, 32)}, animation: google.maps.Animation.DROP})\r\n  End if\r\n\r\n  Set vPoint = nothing\r\nEnd function\r\n\r\n'\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u6309\u9215\r\nfunction rbName_onchange()\r\n' \u8a2d\u5b9a\u70ba\u8f38\u5165\u540d\u7a31\r\n  txtShowGet.value = c_Name(rbName.value())\r\n  localStorage.setItem(\"ShowGet\", txtShowGet.value)\r\nEnd function\r\n\r\n'\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u8a08\u6642\u4ee5\u4f5c\u5132\u5b58\r\nfunction rbName_ontouchstart()\r\n  if rbName.value() = n_NameNum Then\r\n'   \u9023\u7e8c\u6309\u4e0b\u8a08\u6642 (\u5167\u5b9a\u70ba 1500, 1.5 \u79d2)    \r\n    n_TouchTimer = SetTimeout(p_SetName, 1500)\r\n  End if\r\n  n_NameNum = rbName.value()\r\nEnd function\r\n\r\n'\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u8a08\u6642\u505c\u6b62\r\nfunction rbName_ontouchend()\r\n  ClearTimeout(n_TouchTimer)\r\nEnd function\r\n\r\n'\u6309\u4e0b\u540d\u7a31\u8a18\u61b6\u6309\u9215\u540c\u6642\u6309\u4e0b Ctrl \u9375 (PC \u7db2\u9801\u4e0a) (\u7b49\u540c\u65bc\u9577\u6309\u540d\u7a31\u8a18\u61b6\u6309\u9215)\r\nfunction rbName_onclick(e)\r\n  if e.ctrlKey Then\r\n    n_NameNum = rbName.value()\r\n    Call p_SetName\r\n  End if\r\nEnd function\r\n\r\n'\u8a2d\u5b9a\u540d\u7a31\u8a18\u61b6\u6309\u9215\u4e4b\u540d\u5b57\r\nfunction p_SetName()\r\n  if c_Name(n_NameNum) = null Then c_Name(n_NameNum) = \"\"\r\n  c_Name(n_NameNum) = InputBox(v_ChineseInp & CStr(n_NameNum), \"\", c_Name(n_NameNum))\r\n  if c_Name(n_NameNum) = null Then c_Name(n_NameNum) = \"\"\r\n  c_Name(n_NameNum) = Trim(c_Name(n_NameNum))\r\n  if Len(c_Name(n_NameNum)) > 20 Then c_Name(n_NameNum) = Left(c_Name(n_NameNum), 20) \r\n  localStorage.setItem(\"Name\" & n_NameNum, c_Name(n_NameNum))\r\nEnd Sub  \r\n\nfunction txtNameSend_onchange()\n  localStorage.setItem(\"NameSend\", txtNameSend.value)\r\nEnd function\n\nfunction txtShowGet_onchange()\n  localStorage.setItem(\"ShowGet\", txtShowGet.value)\r\nEnd function\n\nfunction txtPassword_onchange()\r\n  localStorage.setItem(\"PassWord\", txtPassword.value)\r\nEnd function\r\n\r\nfunction p_ResetBT()\r\n  rbAuto.disabled = False\r\n  btSend.disabled = False\r\n  btShow.disabled = False\r\nEnd function\r\n", 
